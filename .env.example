# CalorieScan Environment Variables
# Copy this file to .env and fill in your actual API keys

# Clarifai API (for food recognition)
# Get your key at: https://www.clarifai.com/
CLARIFAI_API_KEY=your_clarifai_api_key_here

# Google Cloud Vision API
# Get credentials at: https://cloud.google.com/vision
GOOGLE_CLOUD_PROJECT_ID=your_project_id
GOOGLE_APPLICATION_CREDENTIALS=path/to/service-account-key.json

# Nutritionix API (for nutrition data)
# Sign up at: https://www.nutritionix.com/business/api
NUTRITIONIX_API_ID=your_nutritionix_app_id
NUTRITIONIX_API_KEY=your_nutritionix_api_key

# USDA FoodData Central API (alternative nutrition data source)
# Get key at: https://fdc.nal.usda.gov/api-key-signup.html
USDA_API_KEY=your_usda_api_key

# Custom API endpoint (if you're hosting your own ML model)
CUSTOM_ML_API_URL=https://your-api-endpoint.com
CUSTOM_ML_API_KEY=your_api_key
